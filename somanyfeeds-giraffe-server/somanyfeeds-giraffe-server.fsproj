<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>SoManyFeedsServer</AssemblyName>
    <RootNamespace>SoManyFeedsServer</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Giraffe" Version="4.0.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="Logo.fs" />
    <Compile Include="Layout.fs" />
    <Compile Include="HomePage.fs" />
    <Compile Include="WebApp.fs" />
    <Compile Include="Program.fs" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="WebRoot\**\*">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>
