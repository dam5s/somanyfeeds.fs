<!DOCTYPE html>
<html lang="en">
{% include "_head.html.liquid" %}
<body>

<script src="/somanyfeeds.js?v={% include "_assets_version.html.liquid" %}"></script>
<script>
    var app = Elm.SoManyFeeds.Read.init({flags: {
        userName: "{{ model.UserName }}",
        recents: {{ model.RecentsJson }},
        feeds: {{ model.FeedsJson }},
        page: "{{ model.Page }}",
        selectedFeedId : {{ model.SelectedFeedId }}
    }});

    app.ports.redirectTo.subscribe(function (destination) {
        window.location = destination;
    })
</script>

</body>
</html>
